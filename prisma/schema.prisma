generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model ad_cliente {
    id              Int       @id @default(autoincrement())
    nome            String?   @db.VarChar(60)
    posicao         String?   @db.VarChar(45)
    altura          String?   @db.VarChar(20)
    peso            String?   @db.VarChar(45)
}
